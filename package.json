{
  "name": "taro-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Taro starter template with typescript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/taro-starter.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "author": "funnyzak <silenceace@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnyzak/taro-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/taro-starter#readme",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "eslint:check": "eslint --max-warnings 100 ./ --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint --fix ./ --ext .js,.jsx,.ts,.tsx",
    "prettier:check": "prettier --check './**/*.{ts,js,jsx,tsx}'",
    "prettier:fix": "prettier --write './**/*.{ts,js,jsx,tsx}'",
    "lint:check": "pnpm eslint:check && pnpm prettier:check",
    "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
    "browser:update": "browserslist --update-db",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache",
      "git add"
    ],
    "*.{md,html,json,css,less,scss,sass}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "3.6.21",
    "@tarojs/helper": "3.6.21",
    "@tarojs/plugin-framework-react": "3.6.21",
    "@tarojs/plugin-html": "3.6.21",
    "@tarojs/plugin-platform-alipay": "3.6.21",
    "@tarojs/plugin-platform-h5": "3.6.21",
    "@tarojs/plugin-platform-jd": "3.6.21",
    "@tarojs/plugin-platform-qq": "3.6.21",
    "@tarojs/plugin-platform-swan": "3.6.21",
    "@tarojs/plugin-platform-tt": "3.6.21",
    "@tarojs/plugin-platform-weapp": "3.6.21",
    "@tarojs/react": "3.6.21",
    "@tarojs/runtime": "3.6.21",
    "@tarojs/shared": "3.6.21",
    "@tarojs/taro": "3.6.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3.6.21",
    "@tarojs/taro-loader": "3.6.21",
    "@tarojs/test-utils-react": "^0.1.1",
    "@tarojs/webpack5-runner": "3.6.21",
    "@types/jest": "^29.3.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "babel-preset-taro": "3.6.21",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-taro": "3.6.21",
    "eslint-define-config": "2.0.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.18",
    "prettier": "^3.1.1",
    "react-refresh": "^0.11.0",
    "rimraf": "^5.0.5",
    "stylelint": "^14.16.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "webpack": "5.78.0"
  }
}
